{
  "name": "openhack-backend",
  "alias": ["api-openhack.now.sh"],
  "scope": "openhack-team-141",
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node-server"
    }
  ],
  "build": {
    "env": {
      "mongopwd": "@mongopwd"
    }
  },
  "env": {
    "mongopwd": "@mongopwd"
  },
  "routes": [
    {
      "src": "/",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/test",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/users",
      "dest": "src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/user",
      "dest": "src/index.js",
      "methods": ["POST", "PUT", "DELETE"]
    },
    {
      "src": "/user/:id",
      "dest": "src/index.js",
      "methods": ["GET"]
    }
  ]
}